    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=24"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
