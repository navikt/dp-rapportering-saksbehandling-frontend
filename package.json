    "@navikt/oasis": "^4.1.1",
    "@react-router/fs-routes": "^7.10.0",
    "@react-router/node": "^7.10.0",
    "@react-router/serve": "^7.10.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "isbot": "^5.1.32",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.0",
    "tiny-invariant": "^1.3.3",
