{
  "name": "dp-rapportering-saksbehandling-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "cross-env react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint",
    "test": "npm run test:vitest && npm run test:playwright",
    "prepare": "husky",
    "test:vitest": "vitest run",
    "pre-commit": "lint-staged && npm run typecheck",
    "test:playwright": "playwright test --trace on"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@mswjs/data": "^0.16.2",
    "@navikt/aksel-icons": "^7.22.0",
    "@navikt/ds-css": "^7.22.0",
    "@navikt/ds-react": "^7.22.0",
    "@navikt/oasis": "^3.8.0",
    "@react-router/fs-routes": "^7.6.2",
    "@react-router/node": "^7.6.2",
    "@react-router/serve": "^7.6.2",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "isbot": "^5.1.28",
    "msw": "^2.10.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.2",
    "tiny-invariant": "^1.3.3",
    "tinyduration": "^3.4.1",
    "uuidv7": "^1.0.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.1",
    "@react-router/dev": "^7.6.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^22.15.31",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "playwright": "^1.53.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0",
      "bash -c tsc",
      "bash -c 'npm run lint'"
    ]
  }
}
